{
  "name": "@appland/appmap-agent-js",
  "version": "3.0.0",
  "os": [
    "linux",
    "darwin"
  ],
  "engine": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "appmap",
    "appland",
    "javascript",
    "instrumentation",
    "visualization",
    "tracing",
    "recording",
    "testing"
  ],
  "scripts": {
    "coverage": "npx c8 --reporter=html --require $TARGET node test/unit/$TARGET",
    "build": "node build/index.mjs",
    "prettify": "npx prettier --write 'lib/server/**/*.mjs' 'lib/client/**/*.js' 'test/**/*.mjs' 'bin/*.mjs' 'build/*.mjs'",
    "commit": "npx cz",
    "lint": "npx eslint 'lib/client/**/*.js' 'lib/server/**/*.mjs' 'test/**/*.mjs' 'bin/*.mjs' 'build/*.mjs'",
    "test": "node test/unit/index.mjs",
    "test-integration": "node test/integration/index.mjs",
    "deploy": "npm i -D @semantic-release/changelog @semantic-release/git && npx semantic-release"
  },
  "files": [
    "/dist/*.{js,mjs}",
    "/lib/**/*.{js,mjs}",
    "/bin/*.{js,mjs}",
    "/src/*.json"
  ],
  "bin": {
    "appmap-agent-js": "bin/index.mjs"
  },
  "main": "/lib/client/es2015/node14x/recorder/empty-main.js",
  "dependencies": {
    "acorn": "^8.1.1",
    "ajv": "^8.1.0",
    "escodegen": "^2.0.0",
    "glob": "^7.1.7",
    "minimatch": "^3.0.4",
    "minimist": "^1.2.5",
    "net-socket-messaging": "^0.1.5",
    "npm": "^6.14.12",
    "posix-socket": "^1.0.1",
    "posix-socket-messaging": "^0.0.1",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^18.0.0",
    "c8": "^7.6.0",
    "chalk": "^4.1.0",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "jasmine": "^3.7.0",
    "mocha": "^8.4.0",
    "prettier": "^2.2.1",
    "rollup": "^2.45.2",
    "tap": "^15.0.5",
    "tape": "^5.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/applandinc/appmap-agent-js.git"
  },
  "publishConfig": {
    "cache": "~/.npm",
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
