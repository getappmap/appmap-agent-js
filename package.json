{
  "name": "@appland/appmap-agent-js",
  "version": "11.2.0",
  "bin": {
    "appmap-agent-js": "bin/appmap-agent-js"
  },
  "main": "lib/node/recorder-api.mjs",
  "license": "MIT",
  "engines": {
    "node": "14.x.x || 16.x.x || 17.x.x"
  },
  "licence": "MIT",
  "keywords": [
    "appmap",
    "appland",
    "javascript",
    "instrumentation",
    "visualization",
    "tracing",
    "recording",
    "testing"
  ],
  "homepage": "https://github.com/applandinc/appmap-agent-js.git/#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/applandinc/appmap-agent-js.git"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "node build/await/index.mjs && node build/index.mjs",
    "lint": "npx eslint '**/*.@(mjs|js|cjs)'",
    "test-unit-cov": "npx test-turtle --layout alongside -- /bin/sh test/unit/test-cov.sh",
    "test-unit": "npx test-turtle --layout alongside -- node --unhandled-rejections=strict test/unit/test.mjs",
    "test-unit-pass": "npx test-turtle --layout alongside -- node --unhandled-rejections=strict test/unit/test-pass.mjs",
    "test-system": "node --unhandled-rejections=strict test/system/index.mjs",
    "format": "npx prettier -w '**/*.@(mjs|js|cjs)'",
    "deploy": "npx semantic-release",
    "tt": "npx test-turtle --layout alongside -- /bin/sh test/unit/test-dev.sh"
  },
  "optionalDependencies": {
    "posix-socket": "^1.1.3",
    "posix-socket-messaging": "0.0.1"
  },
  "dependencies": {
    "@appland/appmap-validate": "^2.3.0",
    "@babel/parser": "^7.15.8",
    "acorn": "^8.5.0",
    "ajv": "^8.6.3",
    "ajv-error-tree": "0.0.5",
    "astring": "^1.8.3",
    "glob": "^7.2.0",
    "klaw-sync": "^6.0.0",
    "minimatch": "^3.0.4",
    "minimist": "^1.2.5",
    "net-socket-messaging": "^0.1.6",
    "prompts": "^2.4.2",
    "semver": "^7.3.5",
    "source-map": "^0.6.1",
    "treeify": "^1.1.0",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.15.0",
    "@babel/plugin-syntax-top-level-await": "^7.14.5",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "c8": "^7.6.0",
    "eslint": "^8.2.0",
    "eslint-plugin-import": "^2.22.1",
    "express": "^4.17.1",
    "jsdom": "^18.0.1",
    "mocha": "^9.1.3",
    "mysql": "^2.18.1",
    "pg": "^8.6.0",
    "prettier": "^2.2.1",
    "semantic-release": "^18.0.0",
    "sinon": "^12.0.1",
    "sqlite3": "^5.0.2",
    "tap": "^15.0.10",
    "tape": "^5.2.2",
    "test-turtle": "^5.1.1"
  },
  "publishConfig": {
    "cache": "~/.npm",
    "access": "public"
  }
}
